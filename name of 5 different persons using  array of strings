#include <stdio.h>
#include <string.h>

int main() {
    char names[5][50];

    printf("Enter the names of 5 different persons:\n");

    for (int i = 0; i < 5; i++) {
        scanf("%s", names[i]);
    }

    printf("The names of the 5 different persons are:\n");

    for (int i = 0; i < 5; i++) {
        printf("%s\n", names[i]);
    }

    return 0;
}
