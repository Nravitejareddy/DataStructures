#include <stdio.h>

int main() {
    int num1, num2;

    printf("Enter first number: ");
    scanf("%d", &num1);

    printf("Enter second number: ");
    scanf("%d", &num2);

    if (num1 < 0 || num2 < 0) {
        printf("Either number is negative\n");
    } else {
        printf("Both numbers are positive\n");
    }

    return 0;
}
